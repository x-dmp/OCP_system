package cn.edu.cuit.model.entity;

import java.io.Serializable;

/**
 * @auther yincaiTA
 * @date 2020/12/30 20:55
 * @description 权限实体类
 */
public class Permission implements Serializable {
    private static final long serialVersionUID = 6664178796245444749L;

    /**
     * 主键id
     */
    private Long id;

    /**
     * 权限标识
     */
    private String permission;

    /**
     * 权限描述
     */
    private String description;

    @Override
    public String toString() {
        return "Permission{" +
                "id=" + id +
                ", permission='" + permission + '\'' +
                ", description='" + description + '\'' +
                '}';
    }

    public Long getId() {
        return id;
    }

    public Permission setId(Long id) {
        this.id = id;
        return this;
    }

    public String getPermission() {
        return permission;
    }

    public Permission setPermission(String permission) {
        this.permission = permission;
        return this;
    }

    public String getDescription() {
        return description;
    }

    public Permission setDescription(String description) {
        this.description = description;
        return this;
    }
}
