package cn.edu.cuit.model.bo;

import org.springframework.format.annotation.DateTimeFormat;

import java.util.Date;

public class TransferSlipBo {
    private String transferBoNumber;

    @DateTimeFormat(pattern = "yyyy-MM-dd HH:mm:ss")
    private Date startDate;

    @DateTimeFormat(pattern = "yyyy-MM-dd HH:mm:ss")
    private Date endDate;
    private String creator;

    public String getTransferBoNumber() {
        return transferBoNumber;
    }

    public void setTransferBoNumber(String transferBoNumber) {
        this.transferBoNumber = transferBoNumber;
    }

    public String getCreator() {
        return creator;
    }

    public void setCreator(String creator) {
        this.creator = creator;
    }

    public Date getStartDate() {
        return startDate;
    }

    public void setStartDate(Date startDate) {
        this.startDate = startDate;
    }

    public Date getEndDate() {
        return endDate;
    }

    public void setEndDate(Date endDate) {
        this.endDate = endDate;
    }


}
